@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://example.org/> .
@base <http://example.org/> .
:firstTM a rml:TriplesMap ;
    rml:logicalSource [
        rml:source "data.csv";
        rml:referenceFormulation ql:CSV
    ];
    # Done

    rml:subjectMap [
        rml:reference "myFirstVariable" ;
        rr:termType rr:BlankNode
    ];
    # Done

    rr:predicateObjectMap [
        rr:predicate rdf:type ;
        rml:objectMap [
            rml:reference rdf:Statement
        ]
    ];
    # Done

    rr:predicateObjectMap [
        rr:predicate rdf:subject ;
        rml:objectMap [
            rr:template "http://example/{c1}"
        ]
    ];
    # Done

    rr:predicateObjectMap [
        rr:predicate rdf:predicate ;
        rml:objectMap [
            rr:constant ex:p
        ]
    ];
    # Done

    rr:predicateObjectMap [
        rr:predicate rdf:object ;
        rml:objectMap [
            rr:template "http://example/{c3}"
        ]
    ]; .
    # Done

:secondTM a rml:TriplesMap ;
    rml:logicalSource [
        rml:source "data.csv";
        rml:referenceFormulation ql:CSV
    ];
    # Done

    rml:subjectMap [
        rml:reference "mySecondVariable" ;
        rr:termType rr:BlankNode
    ];
    # Done

    rr:predicateObjectMap [
        rr:predicate rdf:type ;
        rml:objectMap [
            rml:reference rdf:Statement
        ]
    ];
    # Done

    rr:predicateObjectMap [
        rr:predicate rdf:subject ;
        rml:objectMap [
            rr:template "http://example/{c4}"
        ]
    ];
    # Done

    rr:predicateObjectMap [
        rr:predicate rdf:predicate ;
        rml:objectMap [
            rr:constant ex:q
        ]
    ];
    # Done

    rr:predicateObjectMap [
        rr:predicate rdf:object ;
        rml:objectMap [
            rr:parentTriplesMap :firstTM
        ]
    ]; .
    # Done

:thirdTM a rml:TriplesMap ;
    rml:logicalSource [
        rml:source "data.csv";
        rml:referenceFormulation ql:CSV
    ];
    # Done

    rml:subjectMap [
        rml:reference "myThirdVariable" ;
        rr:termType rr:BlankNode
    ];
    # Done

    rr:predicateObjectMap [
        rr:predicate rdf:type ;
        rml:objectMap [
            rml:reference rdf:Statement
        ]
    ];
    # Done

    rr:predicateObjectMap [
        rr:predicate rdf:subject ;
        rml:objectMap [
             rr:parentTriplesMap :secondTM
        ]
    ];
    # Done

    rr:predicateObjectMap [
        rr:predicate rdf:predicate ;
        rml:objectMap [
            rr:constant ex:r
        ]
    ];
    # Done

    rr:predicateObjectMap [
        rr:predicate rdf:object ;
        rml:objectMap [
            rr:template "http://example/{c7}"
        ]
    ]; .
    # Done
